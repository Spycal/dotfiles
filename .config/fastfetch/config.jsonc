      "format": "\u001b[1;90m───────────────────────"
    },
    {
      "type": "os",
      "key": ""
    },
    {
      "type": "packages",
      "key": ""
    },
    {
      "type": "wm",
      "key": "󰖯"
    },
    {
      "type": "uptime",
      "key": "󰳡"
    },
    {
      "type": "terminal",
      "key": ""
    },
    {
      "type": "custom",
      "format": "\u001b[1;90m───────────────────────"
    },
    {
      "type": "colors",
      "paddingLeft": 4,
      "symbol": "circle"
    }
  ]
}
